set mainfont {Iconsolata 14}
set textfont {Monaco 14}
set uifont {Iconsolata 12}
set tabstop 8
set findmergefiles 0
set maxgraphpct 50
set maxwidth 16
set cmitmode patch
set wrapcomment none
set autoselect 1
set showneartags 1
set hideremotes 0
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set want_ttk 1
set colors {green red blue magenta darkgrey brown orange}
set diffcontext 3
set extdifftool opendiff
set perfile_attrs 0
set geometry(main) 1000x750+496+183
set geometry(state) normal
set geometry(topwidth) 1000
set geometry(topheight) 256
set geometry(pwsash0) "536 1"
set geometry(pwsash1) "792 1"
set geometry(botwidth) 705
set geometry(botheight) 489
set permviews {}
